    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/leaflet": "^1.9.21",
    "@types/nodemailer": "^6.4.17",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "critters": "^0.0.23",
    "dayjs": "^1.11.19",
    "dotenv": "^17.3.1",
