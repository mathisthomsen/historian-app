# Staging Environment Configuration
NODE_ENV=staging

# Database (Neon/Postgres)
DATABASE_URL=postgres://youruser:yourpassword@yourhost:5432/yourdb
DATABASE_URL_UNPOOLED=postgres://youruser:yourpassword@yourhost:5432/yourdb
DATABASE_HOST=yourhost
DATABASE_PORT=5432
DATABASE_NAME=yourdb
DATABASE_USERNAME=youruser
DATABASE_PASSWORD=yourpassword

# JWT
JWT_SECRET=staging_jwt_secret_key_change_in_production

# App Configuration
NEXT_PUBLIC_APP_URL=https://staging.historian-app.com

# Strapi Configuration
NEXT_PUBLIC_STRAPI_URL=https://staging.historian-app.com
APP_KEYS=key1,key2,key3,key4
ENCRYPTION_KEY=your_encryption_key_here
ADMIN_JWT_SECRET=your_admin_jwt_secret_here
ADMIN_API_TOKEN_SALT=your_admin_api_token_salt_here
TRANSFER_TOKEN_SALT=your_transfer_token_salt_here

# WorkOS integration
WORKOS_API_KEY=your-workos-api-key-here
WORKOS_CLIENT_ID=your-workos-client-id-here
WORKOS_REDIRECT_URI=https://staging.historian-app.com/api/auth/callback
WORKOS_COOKIE_PASSWORD=your-workos-cookie-password-here
AUTHKIT_REDIRECT_URI=https://staging.historian-app.com/api/auth/callback

# Redis
REDIS_PASSWORD=your_redis_password_here
REDIS_URL=redis://:your_redis_password_here@redis:6379

# SSL and Domain
DOMAIN=staging.historian-app.com
SSL_EMAIL=your-email@example.com 